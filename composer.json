{
    "name": "corals/cms",
    "description": "CMS",
    "keywords": [
        "corals",
        "CMS"
    ],
    "homepage": "https://github.com/coralsio/cms",
    "license": "MIT",
    "authors": [
        {
            "name": "corals",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Corals\\Modules\\CMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "laravel/pint": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Corals\\Modules\\CMS\\CMSServiceProvider"
            ]
        }
    },
    "require": {
        "corals/utility-comment": "^10.0.0",
        "corals/utility-seo": "^10.0.0",
        "corals/utility-tag": "^10.0.0",
        "cviebrock/eloquent-sluggable": "^11.0.0|^10.0.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.laraship.com"
        }
    ],
    "minimum-stability": "dev"
}
